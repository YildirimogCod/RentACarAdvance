{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RentACar": "Server=.;Database=MyDb;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "CacheSettings": {
    "SlidingExpiration": 2
  },
  "SerilogConfigurations": {
    "FileConfiguration": {
      "FilePath": "/logs/"
    }
  },
  "AllowedHosts": "*"
}
